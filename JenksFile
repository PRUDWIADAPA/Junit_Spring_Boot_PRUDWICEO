pipeline {
    agent{
        docker{
            image:'maven:3.9.8-eclipse-temurin-21'
            reuseNode: true
        }
    }

    stages {
        stage('Build') {
            steps {
                echo 'BUILDING THE PROJECT PRUDWI CEO'

                sh 'mvn clean package -DskipTests'
            }
        }
    }
}
